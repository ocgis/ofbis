# 1999-01-18 CG
OFBIS_MAJOR_VERSION=0
OFBIS_MINOR_VERSION=1
OFBIS_MICRO_VERSION=0

# Don't change this
OFBIS_VERSION=$OFBIS_MAJOR_VERSION.$OFBIS_MINOR_VERSION.$OFBIS_MICRO_VERSION
OFBIS_VERSION_CODE=`expr $OFBIS_MAJOR_VERSION*1000000+$OFBIS_MINOR_VERSION*1000+$OFBIS_MICRO_VERSION`

# For automake.
VERSION=$OFBIS_VERSION
PACKAGE=ofbis
